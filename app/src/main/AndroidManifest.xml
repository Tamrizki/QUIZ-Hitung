        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Design.Light.NoActionBar"
        android:usesCleartextTraffic="true">
        <activity android:name=".activity.reward.RewardActivity"></activity>
        <activity android:name=".activity.learnPage.LearnActivity" />
        <activity android:name=".activity.guidePage.GuideActivity" />
        <activity android:name=".activity.gamePlay.GamePlayActivity" />
        <activity android:name=".activity.homePage.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>